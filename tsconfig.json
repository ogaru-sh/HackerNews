    "target": "ES2019",
    "module": "commonjs",
    "strict": true,
    "sourceMap": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
 "include": [
    "./src/**/*"
 ],
 "exclude": [
    "node_modules"
 ]
}
